{
  "name": "upgradeable-example",
  "version": "1.0.0",
  "description": "Example of upgradeable SC",
  "main": "hardhat.config.js",
  "scripts": {
    "compile": "hardhat compile",
    "coverage": "hardhat coverage",
    "docs": "oz-docs",
    "docs:watch": "npm run docs watch contracts 'docs/*.hbs' docs/helpers.js",
    "lint": "npm run lint:js && npm run lint:sol",
    "lint:fix": "npm run lint:js:fix && npm run lint:sol:fix",
    "lint:js": "eslint --ignore-path .gitignore .",
    "lint:js:fix": "eslint --ignore-path .gitignore . --fix",
    "lint:sol": "solhint contracts/**/*.sol && prettier -c contracts/**/*.sol",
    "lint:sol:fix": "prettier --write \"contracts/**/*.sol\"",
    "clean": "hardhat clean && rimraf build contracts/build",
    "prepare": "npm run clean && npm run compile",
    "test": "hardhat test",
    "gas-report": "npm run test",
    "slither": "npm run clean && slither . --detect reentrancy-eth,reentrancy-no-eth,reentrancy-unlimited-gas"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BhHaipls/upgradeable-example.git"
  },
  "keywords": [
    "Solidity",
    "HardHat",
    "SmartContract"
  ],
  "author": "Applicature",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BhHaipls/upgradeable-example/issues"
  },
  "homepage": "https://github.com/BhHaipls/upgradeable-example#readme",
  "dependencies": {
    "@openzeppelin/contracts": "^4.6.0",
    "@openzeppelin/contracts-upgradeable": "^4.6.0",
    "ethers": "5.6.0",
    "mocha": "^9.2.2"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-truffle5": "^2.0.5",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/docs-utils": "^0.1.0",
    "@openzeppelin/hardhat-upgrades": "^1.17.0",
    "@openzeppelin/test-helpers": "^0.5.15",
    "@types/chai": "^4.2.18",
    "@types/chai-as-promised": "^7.1.4",
    "@types/jest": "^27.4.1",
    "@types/mocha": "^8.2.3",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-bignumber": "^3.0.0",
    "chai-bn": "^0.3.0",
    "dotenv": "^16.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "hardhat": "^2.9.2",
    "hardhat-contract-sizer": "^2.5.1",
    "hardhat-gas-reporter": "^1.0.8",
    "prettier": "^2.3.0",
    "prettier-plugin-solidity": "^1.0.0-beta.16",
    "solhint": "^3.3.7",
    "solidity-coverage": "^0.7.20",
    "solidity-docgen": "^0.5.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "yargs": "^17.0.0"
  }
}
